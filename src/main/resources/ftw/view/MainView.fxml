    <center>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
        <children>
            <LineChart fx:id="lineChart" layoutX="70.0" layoutY="20.0" prefHeight="342.0" prefWidth="500.0">
                <xAxis>
                    <CategoryAxis side="BOTTOM" />
                </xAxis>
                <yAxis>
                    <NumberAxis side="LEFT" />
                </yAxis>
            </LineChart>
            <Button fx:id="createStrategyButton" layoutX="102.0" layoutY="361.0" mnemonicParsing="false" text="Add strategy" />
        </children>
    </AnchorPane>
    </center>
</BorderPane>
